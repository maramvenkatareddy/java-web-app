pipeline {
    agent any
    triggers {
        pollSCM('* * * * *')
    }
    environment {
                // Define the target repository in Artifactory
                TARGET_REPO = 'java1-libs-release-local'
            }
    stages {
        stage('Clone the code from GitHub') {
            steps {
                git branch: 'sonar', url: 'https://github.com/maramvenkatareddy/java-web-app.git'
            }
        }
        stage('build') {
            steps {
                sh 'mvn package'
            }
        }
       
    }
}
