pipeline {
  agent any
  parameters {
    choice(name: 'CHOICE', choices: ['maven_3.6', 'maven3.9'], description: 'pick up maven version')
  }
  tools {
        maven 'maven_3.6'
        maven 'maven3.9'
    }
  stages {
    stage('clone the application code') {
      steps {
        git branch: 'sonar', url: 'https://github.com/maramvenkatareddy/java-web-app.git'
      }
    }
    stage('build the application code usingmaven tool') {
      steps {
        sh 'mvn package'
      }
    }
  }
}
