pipeline {
  agent any
  tools {
        maven 'maven_3.6'
    }
  stages {
    stage('clone the application code') {
      steps {
        git branch: 'sonar', url: 'https://github.com/maramvenkatareddy/java-web-app.git'
      }
    }
    stage('build the application code usingmaven tool') {
      steps {
        sh 'mvn package'
      }
    }
    stage('maven version') {
      tools {
        maven 'maven3.9'
      }
      steps {
        sh 'mvn --version'
      }
    }
  }
}
